{
  "name": "yyt-28-server",
  "version": "1.0.0",
  "description": "Stateful WebSocket server",
  "main": "src/main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node src/main.ts",
    "prebuild": "prisma generate",
    "build": "rm -rf dist && tsc",
    "db:update": "prisma migrate dev",
    "db:export": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script",
    "docker:build": "docker build -t lacti/yyt-28-server .",
    "docker:push": "docker push lacti/yyt-28-server"
  },
  "author": "jaeyoung.choi",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/swagger": "^8.2.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/websocket": "^7.1.1",
    "@prisma/client": "^4.7.1",
    "@sinclair/typebox": "^0.25.11",
    "cuid": "^2.1.8",
    "date-fns": "^2.29.3",
    "fastify": "^4.10.2",
    "serialize-error": "^9.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
